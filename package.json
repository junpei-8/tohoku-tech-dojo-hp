{
	"name": "tohoku-tech-dojo-hp",
	"version": "1.0.0",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"serve": "node build/index.js",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"smui-theme:prepare": "pnpm smui-theme-light && pnpm smui-theme-dark",
		"smui-theme:light": "smui-theme compile static/smui.css -i src/theme ./node_modules",
		"smui-theme:dark": "smui-theme compile static/smui-dark.css -i src/theme/dark ./node_modules",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@neoconfetti/svelte": "^1.0.0",
		"@playwright/test": "^1.25.0",
		"@smui/button": "7.0.0-beta.0",
		"@sveltejs/adapter-node": "1.0.0-next.98",
		"@sveltejs/kit": "next",
		"@sveltestack/svelte-query": "^1.6.0",
		"@types/cookie": "^0.5.1",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.6.2",
		"prettier-plugin-jsdoc": "^0.4.2",
		"prettier-plugin-svelte": "^2.7.0",
		"sass": "^1.55.0",
		"smui-theme": "7.0.0-beta.0",
		"svelte": "^3.46.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.6",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^3.1.0"
	},
	"type": "module"
}
