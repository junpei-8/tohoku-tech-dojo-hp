{
	"name": "tohoku-tech-dojo-hp",
	"version": "1.0.0",
	"scripts": {
		"install:svelte": "pnpm install && svelte-kit sync",
		"i:svelte": "pnpm install:svelte",
		"dev": "vite dev",
		"dev:serve": "pnpm build && pnpm serve",
		"build": "vite build",
		"serve": "node build/index.js",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"svelte:watch": "pnpm check:watch",
		"smui-theme:light": "smui-theme compile static/smui.css -i src/theme ./node_modules",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"dependencies": {
		"@smui/button": "7.0.0-beta.0",
		"@smui/card": "7.0.0-beta.0",
		"@smui/circular-progress": "7.0.0-beta.0",
		"@smui/ripple": "7.0.0-beta.0"
	},
	"devDependencies": {
		"@neoconfetti/svelte": "^1.0.0",
		"@sveltejs/adapter-node": "1.0.0-next.100",
		"@sveltejs/kit": "1.0.0-next.531",
		"@sveltestack/svelte-query": "^1.6.0",
		"@types/cookie": "^0.5.1",
		"@typescript-eslint/eslint-plugin": "^5.42.0",
		"@typescript-eslint/parser": "^5.42.0",
		"eslint": "^8.26.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.7.1",
		"prettier-plugin-jsdoc": "^0.4.2",
		"prettier-plugin-svelte": "^2.8.0",
		"sass": "^1.55.0",
		"smui-theme": "7.0.0-beta.0",
		"svelte": "^3.52.0",
		"svelte-check": "^2.9.2",
		"svelte-preprocess": "^4.10.7",
		"tslib": "^2.4.1",
		"typescript": "^4.8.4",
		"vite": "^3.2.2"
	},
	"type": "module",
	"license": "MIT"
}
